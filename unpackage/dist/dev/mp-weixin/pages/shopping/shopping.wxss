  color: #333333;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.goods_list .goods .goods_detail .title_box .specifications {
  font-size: 20rpx;
  line-height: 28rpx;
  padding: 2rpx 12rpx;
