  background: #f7f3f1;
  border-radius: 6rpx;
  border: 1rpx solid #d8d8d8;
}
.goods_list .goods .goods_detail .price {
  font-family: Akrobat-Regular;
  font-size: 40rpx;
  line-height: 48rpx;
