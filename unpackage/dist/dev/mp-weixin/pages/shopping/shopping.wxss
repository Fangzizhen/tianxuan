.page {
  width: 100%;
  height: 100%;
}
.goods_list {
  background-color: #fff;
  margin-top: 18rpx;
}
.goods_list .goods {
  height: 254rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.goods_list .goods .label {
  margin-left: 34rpx;
}
