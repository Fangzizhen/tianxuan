  margin-left: 6rpx;
}
.goods_list .goods .goods_detail .price .earn text {
  color: #FE7956;
  margin-left: 4rpx;
  font-family: Akrobat-Regular;
  font-size: 40rpx;
  line-height: 48rpx;
}
.goods_list .goods .goods_detail .number {
  position: absolute;
